<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>LabelServlet</servlet-name>
        <servlet-class>com.fpt.hth.bts.controller.LabelServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MilestoneServlet</servlet-name>
        <servlet-class>com.fpt.hth.bts.controller.MilestoneServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LabelServlet</servlet-name>
        <url-pattern>/label</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MilestoneServlet</servlet-name>
        <url-pattern>/milestone</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>home</welcome-file>
    </welcome-file-list>
</web-app>
